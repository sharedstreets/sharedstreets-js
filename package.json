{
  "name": "sharedstreets",
  "version": "0.11.0",
  "description": "SharedStreets, a 'digital commons' for the street",
  "main": "src/index",
  "bin": {
    "shst": "./bin/run"
  },
  "files": [
    "build/index.js",
    "src/index.ts"
  ],
  "oclif": {
    "commands": "./src/commands",
    "bin": "shst",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "pretest": "tsc",
    "test-core": "tsc && colortape build/test_core.js",
    "test-matcher": "tsc && colortape build/test_matcher.js",
    "test-graph": "tsc && colortape build/test_graph.js",
    "docs": "tsc && documentation readme build/index.js --section=API",
    "bench": "tsc && node bench.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharedstreets/sharedstreets.js.git"
  },
  "keywords": [
    "sharedstreets",
    "streets",
    "highway",
    "reference"
  ],
  "author": "SharedStreets",
  "contributors": [
    "Denis Carriere <@DenisCarriere>",
    "Kevin Webb <@kpwebb>",
    "Morgan Herlocker <@morganherlocker>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sharedstreets/sharedstreets.js/issues"
  },
  "homepage": "https://github.com/sharedstreets/sharedstreets.js#readme",
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.3",
    "@types/benchmark": "*",
    "@types/glob": "*",
    "@types/node": "*",
    "@types/tape": "*",
    "benchmark": "^2.1.4",
    "colortape": "^0.1.2",
    "documentation": "^9.3.1",
    "sharedstreets-pbf": "*",
    "tap": "*",
    "tape": "*",
    "tslint": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@hapi/basic": "^5.1.0",
    "@hapi/inert": "^5.2.0",
    "@mapbox/sphericalmercator": "^1.1.0",
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.12.11",
    "@oclif/plugin-help": "^2.1.6",
    "@turf/along": "^6.0.1",
    "@turf/bbox": "^6.0.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/bearing": "^6.0.1",
    "@turf/boolean-point-in-polygon": "^6.0.1",
    "@turf/buffer": "^5.1.5",
    "@turf/envelope": "^5.1.5",
    "@turf/helpers": "^6.0.1",
    "@turf/invariant": "^6.0.1",
    "@turf/length": "^6.0.1",
    "@turf/line-offset": "^5.1.5",
    "@turf/line-slice-along": "^5.1.5",
    "@turf/nearest-point-on-line": "^6.0.2",
    "@types/leveldown": "^4.0.0",
    "@types/levelup": "^3.1.0",
    "@types/osrm": "^5.12.0",
    "bignumber.js": "6.x",
    "chalk": "^2.4.2",
    "geojson-rbush": "^3.1.1",
    "hapi": "^18.1.0",
    "jkstra": "^0.0.6",
    "leveldown": "^5.0.0",
    "levelup": "^4.0.1",
    "moment-timezone": "^0.5.23",
    "node-fetch": "^2.3.0",
    "osrm": "^5.22.0",
    "sharedstreets-types": "^1.3.1",
    "simple-statistics": "^7.0.2",
    "uuid-by-string": "^2.1.0",
    "xml": "^1.0.1"
  }
}
